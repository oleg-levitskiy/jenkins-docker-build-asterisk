#!groovy
// Run docker build
properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'node-114'
        }
triggers { pollSCM('* * * * *') }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
stages {
        stage("docker login") {
            steps {
                echo " ============== docker login =================="
                  }
         }
     stage("docker build") {
            steps {
                echo " ============== docker build =================="
                dir ('./') {
                	sh 'docker build -t telworks/aster-16-jenkinsbuild:latest . '
                }
                  }
         
         }
}     
   }
     
    
